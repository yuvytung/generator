<%
const Entity = _entity.name.pascal().concat();
const EntityRepository = _entity.name.pascal().concat("Repository");
const EntityRepositoryImpl = _entity.name.pascal().concat("RepositoryImpl");
-%>

import { Injectable } from "@nestjs/common";
import { InjectModel } from "@nestjs/mongoose";
import { Model } from "mongoose";
import { <%-Entity%> } from "../../domain";
import { BaseRepository } from "../../common/bases";
import { EntitiesEnum } from "../../common/enums/EntitiesEnum";
import { <%-EntityRepository%> } from "..";

@Injectable()
export class <%-EntityRepositoryImpl%> extends BaseRepository<<%-Entity%>> implements <%-EntityRepository%> {
  constructor(@InjectModel(EntitiesEnum.<%-Entity%>) protected readonly model: Model<<%-Entity%>>) {
    super();
  }
}

export default { provide: <%-EntityRepository%>, useClass: <%-EntityRepositoryImpl%> };
