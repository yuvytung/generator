{
  "parser": "esprima",
  "extends": [
    "eslint:recommended",
    "prettier",
    "eslint-config-prettier"
  ],
  "globals": {
    "global": true,
    "process": true,
    "require": true,
    "console": true,

    "YAML": true,
    "log": true,
    "app": true,
    "APP_ENV": true
  },
  "rules": {
    "coffee/no-return-await": "off",
    "coffee/vars-on-top": "off"
  }
}
