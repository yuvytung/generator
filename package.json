{
  "name": "generator",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "description": "project generate code.",
  "scripts": {
    "postinstall": "husky install",
    "start": "node src/main/javascript/index.mjs",
    "render": "node src/main/javascript/index.mjs render ",
    "entity:render": "node src/main/javascript/index.mjs entity_render ",
    "convert": "node src/main/javascript/index.mjs convert",
    "lint": "eslint 'src/main/javascript/' --fix --ext .mjs",
    "prettier": "prettier --write 'src/main/javascript/**/*.mjs'",
    "clean": "rm -rf build",
    "test": "npm run clean && npm run render && chmod +x src/main/bash/main.sh && ./src/main/bash/main.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,apps,libs}/**/*.mjs": [
      "npm run lint",
      "npm run prettier"
    ]
  },
  "dependencies": {
    "case": "^1.6.3",
    "ejs": "^3.1.8",
    "glob": "^8.0.3",
    "log4js": "^6.6.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@nerevu/prettier-coffee": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "coffeescript": "^2.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-coffee": "github:helixbass/eslint-plugin-coffee#eslint-plugin-coffee-v0.1.16-dev.4-gitpkg",
    "eslint-plugin-coffeescript": "^1.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^10.5.4",
    "prettier": "github:helixbass/prettier#prettier-v2.1.0-dev.100-gitpkg",
    "prettier-plugin-coffeescript": "^0.1.5"
  },
  "main": "src/main/javascript"
}
